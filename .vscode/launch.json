  "version": "0.2.0",
  "configurations": [
    {
      "name": "tabtoy debug",
      "type": "go",
      "request": "launch",
      "mode": "auto",
      "program": "${workspaceFolder}",
      "args": [
        "--mode=v2",
        "--json_out=${workspaceFolder}/debug/Config.json",
        "--go_out=${workspaceFolder}/debug/table_gen.go",
        "--proto_out=${workspaceFolder}/debug/pb.proto",
        "--mark=Client",
        "--tiled_dir=${workspaceFolder}/v2/example/tiled/*.tsx",
        "${workspaceFolder}/v2/example/excel/global/global.xlsx",
        "${workspaceFolder}/v2/example/excel/global/RangeType.xlsx",
        "${workspaceFolder}/v2/example/excel/global/Skill.xlsx"

        //"${workspaceFolder}/v2/example/Globals.xlsx",
        //"${workspaceFolder}/v2/example/Sample.xlsx",
        //"${workspaceFolder}/v2/example/AAA.xlsx"
      ]
    }
  ]
}
